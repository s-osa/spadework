# coding: utf-8
$LOAD_PATH.unshift(File.expand_path(File.dirname(__FILE__)))
require 'spadework/order'

puts Order::Base::Version

ImportDir = File.dirname(File.expand_path(__FILE__)) + "/../"

Dir.chdir ImportDir

Dir.glob("*").each do |dir|
  files = Dir.glob("#{dir}/*")
  case dir
  when "config"
    puts "config"
  when "amazon"
    puts "amazon"
  when "yahoo"
    puts "yahoo"
  else
    puts "rakuten"
  end
  puts files
  puts "==========================="
end
